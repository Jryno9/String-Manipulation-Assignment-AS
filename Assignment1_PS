DECLARE inpstr, str1 As String

DECLARE count As Integer

DECLARE cmpchar As Char

DECLARE Notfound As Boolean



inpstr = ""

str1 = "abcdefghijklmnopqrstuvwxyz"

count = 0

Notfound = False
Cmpchar=’’



INPUT ("Enter the string : "),Inspstr


For count = 1 To Len(inpstr)

cmpchar = Mid(inpstr, count, 1)

If LOCATE(str1, LCase(cmpchar)) = 0 Then

Notfound = True

Exit For

Else

Notfound = False

End If



Next

If Notfound = True Then

INPUT("Does not contain all alphabets ")

Else

INPUT("Does contain all alphabets")

End If
